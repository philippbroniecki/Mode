modus <- function(x) {
# sort frequency table in descending order and extract its names
modus <- base::table(... = x) %>%
base::sort(decreasing = TRUE) %>%
base::names()
# x is numeric
if ( base::is.numeric(x) ) {
# coerce character to to numeric
modus <- base::as.numeric(modus)
}
# return mode
return(modus[1])
}
